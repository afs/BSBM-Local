# Source this file.

SIZES=

if [ -z "$SIZES" -a -e "db-sizes" ]
then
    SIZES="$(cat db-sizes)"
fi

## $DATADIR/data-<SIZE>/
DATADIR=Data
[ -d "$DATADIR" ] || { echo "Not a directory: $DATADIR" ; exit 1 ; }

JENADB=TDB-DB
[ -d "$JENADB" ] || { echo "Not a directory: $JENADB" ; exit 1 ; }

. ./run_classpath
